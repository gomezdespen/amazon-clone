ch ({\n            type: 'REMOVE_FROM_BASKET',\n            id: id,\n    })\n    }\n\n    return (\n        <div className=\"checkoutProduct\">\n             <img className=\"checkoutProduct_image\" src={image} alt=\"#\"/>\n             <div className=\"checkoutProduct_info\">\n                <p className=\"checkoutProduct_title\">{title}</p>\n                <p className=\"checkoutProduct_price\"> \n                    <small>$</small>\n                    <strong>{price}</strong>\n                </p>\n                <div className='checkoutProduct_rating'>\n\n                    {Array (rating)\n                    .fill()\n                    .map((_, i) =>(\n                        <p>‚≠ê</p>\n\n                    ))}\n                </div>\n                \n                    <button onClick={removeFromBasket}>Remove from Basket</button>\n             </div> \n        </div>\n    )\n}\n\nexport default CheckoutProduct;\n","/Users/danielgomez/dev/Projects/react/amazon-clone/src/Login.js",[],"/Users/danielgomez/dev/Projects/react/amazon-clone/src/firebase.js",[],{"ruleId":"68","replacedBy":"69"},{"ruleId":"70","replacedBy":"71"},{"ruleId":"72","severity":1,"message":"73","line":14,"column":9,"nodeType":"74","messageId":"75","endLine":14,"endColumn":11},{"ruleId":"76","severity":1,"message":"77","line":36,"column":6,"nodeType":"78","endLine":36,"endColumn":8,"suggestions":"79"},{"ruleId":"80","severity":1,"message":"81","line":10,"column":29,"nodeType":"82","messageId":"83","endLine":10,"endColumn":37},{"ruleId":"80","severity":1,"message":"81","line":10,"column":24,"nodeType":"82","messageId":"83","endLine":10,"endColumn":32},{"ruleId":"84","severity":1,"message":"85","line":29,"column":25,"nodeType":"86","messageId":"87","endLine":29,"endColumn":87},{"ruleId":"80","severity":1,"message":"88","line":6,"column":13,"nodeType":"82","messageId":"83","endLine":6,"endColumn":19},"no-native-reassign",["89"],"no-negated-in-lhs",["90"],"no-empty-pattern","Unexpecte